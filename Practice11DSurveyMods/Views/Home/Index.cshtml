@{
    ViewData["Title"] = "Home Page";
}
@model Survey
<body>
    <form action="/result/process" method="post">
        <div class="mb-3">
            <label asp-for="Name">Name</label>
            <input asp-for="Name" type="text">
        </div>
        <div class="mb-3">
            <label asp-for="Location">Location</label>
            <input asp-for="Location" type="text">
        </div>
        <div class="mb-3">
            <label asp-for="Language">Language</label>
            <input asp-for="Language" type="text">
        </div>
        <div class="mb-3">
            <label asp-for="Comment">Comment</label>
            <input asp-for="Comment" type="text">
        </div>
        <input type="submit" value="Send">
    </form>
</body>
